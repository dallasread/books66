<h1>Listing notes</h1>

<table>
  <tr>
    <th>Story</th>
    <th>User</th>
    <th>Body</th>
    <th>Ordinal</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @notes.each do |note| %>
  <tr>
    <td><%= note.story_id %></td>
    <td><%= note.user_id %></td>
    <td><%= note.body %></td>
    <td><%= note.ordinal %></td>
    <td><%= link_to 'Show', note %></td>
    <td><%= link_to 'Edit', edit_note_path(note) %></td>
    <td><%= link_to 'Destroy', note, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Note', new_note_path %>
